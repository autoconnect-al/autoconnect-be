{
  "name": "Strict user/reset-password parity",
  "request": {
    "method": "POST",
    "path": "/user/reset-password",
    "body": {
      "email": "parity@example.com"
    }
  },
  "compare": {
    "status": true,
    "envelope": true,
    "values": true,
    "keys": [
      "success",
      "message",
      "statusCode"
    ],
    "ignorePaths": []
  }
}
